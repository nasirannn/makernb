{
  "crons": [
    {
      "path": "/api/cron/cleanup-expired-credits",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/subscription-credits",
      "schedule": "0 1 * * *"
    }
  ],
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/:path+\/",
      "destination": "/:path+",
      "permanent": true
    }
  ]
}
